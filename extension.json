{ 
  "name": "HummingBird: I2C SPEC testing",
  "apiVersion": "1.0.0",
  "author": "Peggy Lin",
  "version": "1.0.0",
  "description": "Automated Electrical Test for I2C Signal",
  "extensions": {
    "HummingBird": {
      "type": "AnalogMeasurement",
      "entryPoint": "hummingbird_saleae.HummingBird",
      "metrics": {
        "spec": {
          "name": "spec check result (Pass/Fail)",
          "notation": "FAILs",
          "units": "(Refer to /tmp/output_reports/report.html for more details.)"
        },
        "v_high_scl_worst": {
          "name": "SCL voltage at the HIGH level",
          "notation": "V<sub>H,scl</sub>",
          "units": "V"
        },
        "v_low_scl_worst": {
          "name": "SCL voltage at the LOW level",
          "notation": "V<sub>L,scl</sub>",
          "units": "V"
        },
        "v_high_sda_worst": {
          "name": "SDA voltage at the HIGH level",
          "notation": "V<sub>H,sda</sub>",
          "units": "V"
        },
        "v_low_sda_worst": {
          "name": "SDA voltage at the LOW level",
          "notation": "V<sub>L,sda</sub>",
          "units": "V"
        },
        "v_nh_scl_worst": {
          "name": "SCL noise margin at the HIGH level",
          "notation": "V<sub>nh,scl</sub>",
          "units": "V"
        },
        "v_nl_scl_worst": {
          "name": "SCL noise margin at the LOW level",
          "notation": "V<sub>nl,scl</sub>",
          "units": "V"
        },
        "v_nh_sda_worst": {
          "name": "SDA noise margin at the HIGH level",
          "notation": "V<sub>nh,sda</sub>",
          "units": "V"
        },
        "v_nl_sda_worst": {
          "name": "SDA noise margin at the LOW level",
          "notation": "V<sub>nl,sda</sub>",
          "units": "V"
        },
        "t_rise_scl_worst": {
          "name": "SCL max Rise Time",
          "notation": "T<sub>r,scl</sub>",
          "units": "s"
        },
        "t_fall_scl_worst": {
          "name": "SCL max Fall Time",
          "notation": "T<sub>f,scl</sub>",
          "units": "s"
        },
        "t_rise_sda_worst": {
          "name": "SDA max Rise Time",
          "notation": "T<sub>r,sda</sub>",
          "units": "s"
        },
        "t_fall_sda_worst": {
          "name": "SDA max Fall Time",
          "notation": "T<sub>f,sda</sub>",
          "units": "s"
        },
        "t_low_worst": {
          "name": "Minimum low period Time",
          "notation": "T<sub>low</sub>",
          "units": "s"
        },
        "t_high_worst": {
          "name": "Minimum high period Time",
          "notation": "T<sub>high</sub>",
          "units": "s"
        },
        "f_clk_worst": {
          "name": "max clk frequency",
          "notation": "f<sub>CLK</sub>",
          "units": "Hz"
        },
        "t_SU_STA_worst": {
          "name": "RESTART setup time",
          "notation": "T<sub>SU,Sr</sub>",
          "units": "s"
        },
        "t_HD_STA_S_worst": {
          "name": "START hold time",
          "notation": "T<sub>HD,S</sub>",
          "units": "s"
        },
        "t_HD_STA_Sr_worst": {
          "name": "RESTART hold time",
          "notation": "T<sub>HD,Sr</sub>",
          "units": "s"
        },
        "t_SU_DAT_rising_host_worst": {
          "name": "rising data setup time",
          "notation": "T<sub>SU,dat, wr,r</sub>",
          "units": "s"
        },
        "t_SU_DAT_falling_host_worst": {
          "name": "falling data setup time",
          "notation": "T<sub>SU,dat, wr,f</sub>",
          "units": "s"
        },
        "t_HD_DAT_rising_host_worst": {
          "name": "rising data hold time",
          "notation": "T<sub>HD,dat, wr,r</sub>",
          "units": "s"
        },
        "t_HD_DAT_falling_host_worst": {
          "name": "falling data hold time",
          "notation": "T<sub>HD,dat, wr,f</sub>",
          "units": "s"
        },
        "t_SU_DAT_rising_dev_worst": {
          "name": "rising data setup time",
          "notation": "T<sub>SU,dat, rd,r</sub>",
          "units": "s"
        },
        "t_SU_DAT_falling_dev_worst": {
          "name": "falling data setup time",
          "notation": "T<sub>SU,dat, rd,f</sub>",
          "units": "s"
        },
        "t_HD_DAT_rising_dev_worst": {
          "name": "rising data hold time",
          "notation": "T<sub>HD,dat, rd,r</sub>",
          "units": "s"
        },
        "t_HD_DAT_falling_dev_worst": {
          "name": "falling data hold time",
          "notation": "T<sub>HD,dat, rd,f</sub>",
          "units": "s"
        },
        "t_SU_STO_worst": {
          "name": "STOP setup time",
          "notation": "T<sub>SU,STO</sub>",
          "units": "s"
        },
        "t_BUF_worst": {
          "name": "bus free time between STOP and START",
          "notation": "T<sub>BUF</sub>",
          "units": "s"
        }
      }
    }
  }
}
